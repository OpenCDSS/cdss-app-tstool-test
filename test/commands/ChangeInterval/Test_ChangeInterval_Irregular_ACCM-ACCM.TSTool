# Irregular interval to regular, ACCM to ACCM
#
StartLog(LogFile="Results\Test_ChangeInterval_Irregular_ACCM-ACCM.TSTool.log")
RemoveFile(InputFile="Results\Test_ChangeInterval_Irregular_ACCM-ACCM.dv",IfNotFound="Ignore")
ReadDateValue(InputFile="Data\IrregularInput.dv")
ChangeInterval(TSList="LastMatchingTSID",TSID="Input",Alias="Converted",NewInterval="6Hour",OldTimeScale="ACCM",NewTimeScale="ACCM")
ChangeInterval(TSList="LastMatchingTSID",TSID="Input",Alias="Converted_HandleMissing_Repeat",NewInterval="6Hour",OldTimeScale="ACCM",NewTimeScale="ACCM",HandleMissingInputHow="Repeat")
ChangeInterval(TSList="LastMatchingTSID",TSID="Input",Alias="Converted_HandleMissing_SetToZero",NewInterval="6Hour",OldTimeScale="ACCM",NewTimeScale="ACCM",HandleMissingInputHow="SetToZero")
Free(TSList="AllMatchingTSID",TSID="Input")
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults\Test_ChangeInterval_Irregular_ACCM-ACCM.dv",TSList=AllMatchingTSID,TSID="*")
WriteDateValue(TSList="AllMatchingTSID",TSID="*",OutputFile="Results\Test_ChangeInterval_Irregular_ACCM-ACCM.dv")
CompareFiles(InputFile1="ExpectedResults\Test_ChangeInterval_Irregular_ACCM-ACCM.dv",InputFile2="Results\Test_ChangeInterval_Irregular_ACCM-ACCM.dv",IfDifferent="Warn")
