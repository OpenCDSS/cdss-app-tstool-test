StartLog(LogFile="Results/Test_ReadColoradoHydroBaseRest_CompareHydroBase_1_Evap-Min_Month.TSTool.log")
# Test reading 1 Evap-Min month interval time series using ReadColoradoHydroBaseRest:
# - test results against TSID's generated from HydroBase database
RemoveFile(InputFile="Results/Test_ReadColoradoHydroBaseRest_CompareHydroBase_1_Evap-Min_Month_out.dv",IfNotFound=Ignore)
# Read the time series using ReadColoradoHydroBaseRest and from HydroBase.
SetInputPeriod(InputStart="1949-04-06",InputEnd="2018-12-31")
ReadColoradoHydroBaseRest(DataStore="HydroBaseWeb",TSID="USC00053005.NOAA.Evap-Min.Month~HydroBaseWeb")
# USC00053005 - FORT COLLINS
USC00053005.NOAA.EvapPan.Day~HydroBase
NewStatisticMonthTimeSeries(TSID="USC00053005.NOAA.EvapPan.Day",Alias="USC00053005-HydroBase-Month",NewTSID="USC00053005..EvapPan-Min.Month",Statistic=Min)
# Check that missing and nonmissing counts are as expected.
CheckTimeSeriesStatistic(Statistic="NonmissingCount",CheckCriteria="<=",CheckValue1=10,IfCriteriaMet=Warn)
CheckTimeSeriesStatistic(TSList=AllMatchingTSID,TSID="*Month",Statistic="MissingCount",CheckCriteria=">",CheckValue1=316,IfCriteriaMet=Warn)
WriteDateValue(TSList=FirstMatchingTSID,TSID="USC00053005.NOAA.Evap-Min.Month",OutputFile="Results/Test_ReadColoradoHydroBaseRest_CompareHydroBase_Evap-Min_Month_out.dv",Precision=2)
CompareTimeSeries(TSID1="USC00053005.NOAA.Evap-Min.Month",TSID2="USC00053005-HydroBase-Month",Tolerance=".01",IfDifferent=Warn)
