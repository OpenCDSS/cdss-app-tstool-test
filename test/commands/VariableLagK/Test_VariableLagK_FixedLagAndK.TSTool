# Test fixed Lag and attenuation
StartLog(LogFile="Results\Test_VariableLagK_FixedLagAndK.TSTool.log")
RemoveFile(InputFile="Results\Test_VariableLagK_FixedLagAndK_out.dv",IfNotFound=Ignore)
#
# Read NWSCard input file
TS Inflow = ReadNwsCard(InputFile="Data\Streamflow.SQIN")
#
# Route using the same routing parameters used in the mcp3 input deck
# (metric units: Lag(hrs) K(hrs) Q(cms)
# Lag
# K
#     36.
#     22.
VariableLagK(TSID="Inflow",NewTSID="TestLoc..SQIN.6Hour.routed",FlowUnits=CMS,LagInterval=Hour,Lag="0,36",K="0,22",Alias="FixedLagAndK")
# Use the fixed LagK command if necessary to verify differences.
# TS FixedLagAndK_OldLagK = LagK(TSID="Inflow",FillNearest=True,Lag=36,K=22)
Free(TSList=AllMatchingTSID,TSID="Inflow")
#
# Read the output from mcp3 CHANGE-T command
TS mcp3_output = ReadNwsCard(InputFile="Data\FIXED_LAG_AND_K.SQIN")
CompareTimeSeries(MatchLocation=False,MatchDataType=True,Tolerance="0.001",AnalysisStart="1990-12-01",WarnIfDifferent=True)
#
# Once test out the command, use the following to do an exact test of the results:
#
# Uncomment the following command to regenerate the expected results file.
SelectTimeSeries(TSList=AllMatchingTSID,TSID="FixedLagAndK")
# WriteDateValue(OutputFile="ExpectedResults\Test_VariableLagK_FixedLagAndK_out.dv",TSList=SelectedTS)
WriteDateValue(OutputFile="Results\Test_VariableLagK_FixedLagAndK_out.dv",TSList=SelectedTS)
CompareFiles(InputFile1="ExpectedResults\Test_VariableLagK_FixedLagAndK_out.dv",InputFile2="Results\Test_VariableLagK_FixedLagAndK_out.dv",WarnIfDifferent=True)



