# Test checking a time series for values that change from the previous value > an absolute percent
# The expected status is warning because some values are detected.
#@expectedStatus Warning
StartLog(LogFile="Results/Test_CheckTimeSeries_AbsChangePercentGreaterThan.TSTool.log")
RemoveFile(InputFile="Results/Test_CheckTimeSeries_AbsChangePercentGreaterThan_out.html",IfNotFound=Ignore)
RemoveFile(InputFile="Results/Test_CheckTimeSeries_AbsChangePercentGreaterThan_out.csv",IfNotFound=Ignore)
TS ts1 = NewPatternTimeSeries(NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2001-05-13",PatternValues="1,2,3,2,2,-999,5,1,-999,-999,-999,1,3,5")
TS ts2 = NewPatternTimeSeries(NewTSID="ts2...Day",Description="test data 2",SetStart="2002-01-01",SetEnd="2002-02-28",PatternValues="2,4,6,4,2,-999,10,2,-999,-999,-999,2,6,10")
CheckTimeSeries(CheckCriteria="AbsChangePercent>",Value1=190)
# Uncomment the following command to regenerate expected results.
# WriteCheckFile(OutputFile="ExpectedResults/Test_CheckTimeSeries_AbsChangePercentGreaterThan_out.csv")
# Write the html and csv to make sure that no failures result but only
# compare CSV below because the header is in comments that are ignored (html
# is harder to strip out dynamic content like dates in comments).
WriteCheckFile(OutputFile="Results/Test_CheckTimeSeries_AbsChangePercentGreaterThan_out.html")
WriteCheckFile(OutputFile="Results/Test_CheckTimeSeries_AbsChangePercentGreaterThan_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_CheckTimeSeries_AbsChangePercentGreaterThan_out.csv",InputFile2="Results/Test_CheckTimeSeries_AbsChangePercentGreaterThan_out.csv",WarnIfDifferent=True)
