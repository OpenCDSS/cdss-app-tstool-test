# Test setting a property from a table:
# - read the table from the file
# - use IgnoreCaes=False (True is the default)
# - this should result in only 'String:Fourth*' being matched and set to non-null
StartLog(LogFile="Results/test-SetPropertyFromTable.tstool.log")
RemoveFile(InputFile="Results/test-SetPropertyFromTable-out.csv",IfNotFound="Ignore")
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\testtable.csv")
SetPropertyFromTable(TableID="Table1",Column="String2",ColumnIncludeFilters="String:first*",IgnoreCase="False",PropertyName="StringProp1")
SetPropertyFromTable(TableID="Table1",Column="String2",ColumnIncludeFilters="String:Fourth*",PropertyName="StringProp4")
WritePropertiesToFile(OutputFile="Results/test-SetPropertyFromTable-out.txt",IncludeProperties="StringProp*",FileFormat="NameValue",SortOrder="Ascending")
# Uncomment the following to generate the expected results.
# CopyFile(InputFile="Results/test-SetPropertyFromTable-out.txt",OutputFile="ExpectedResults/test-SetPropertyFromTable-out.txt")
# Do regression comparison with the original input
CompareFiles(InputFile1="ExpectedResults/test-SetPropertyFromTable-out.txt",InputFile2="Results/test-SetPropertyFromTable-out.txt",IfDifferent="Warn")
