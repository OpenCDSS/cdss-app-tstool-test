# Test comparing two simple tables using default parameters
# The files are different - generate a command warning
# Check to a precision of of 2 digits
# @expectedStatus Warning
StartLog(LogFile="Results/Test_CompareTables_DifferentTables_Precision_IfDifferent=Warn.TSTool.log")
RemoveFile(InputFile="Results/Test_CompareTables_DifferentTables_Precision_IfDifferent=Warn_out.html",IfNotFound=Ignore)
ReadTableFromDelimitedFile(TableID="table1",InputFile="Data\table1.csv")
ReadTableFromDelimitedFile(TableID="table2",InputFile="Data\table2.2.csv")
# Uncomment to regenerate the expected results...
# Generate the results.
CompareTables(Table1ID="table1",Table2ID="table2",Precision=2,NewTableID="comparison",OutputFile="Results/Test_CompareTables_DifferentTables_Precision_IfDifferent=Warn_out.html",IfDifferent=Warn)
