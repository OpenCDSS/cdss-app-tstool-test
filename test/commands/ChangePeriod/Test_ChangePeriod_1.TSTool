# Test changing the period on a daily time series
StartLog(LogFile="Results/Test_ChangePeriod_1.TSTool.log")
RemoveFile(InputFile="Results/Test_ChangePeriod_1_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1_day",NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2000-05-13",PatternValues="1,2,3,2,1,-999,5,1,-999,-999,-999,1,3,5")
ChangePeriod(TSList=AllTS,NewEnd="2005-07-14")
FillRepeat(TSList=AllMatchingTSID,TSID="*",FillDirection=Forward)
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/Test_ChangePeriod_1_out.dv")
WriteDateValue(OutputFile="Results/Test_ChangePeriod_1_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_ChangePeriod_1_out.dv",InputFile2="Results/Test_ChangePeriod_1_out.dv",IfDifferent=Warn)
