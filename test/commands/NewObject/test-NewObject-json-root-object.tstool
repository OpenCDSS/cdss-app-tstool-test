# Test creating a new object:
# - use a simple JSON input file with root level dictionary
StartLog(LogFile="Results/test-NewObject-json-root-object.tstool.log")
# Create the object from the JSON file.
NewObject(ObjectID="TestObject",InputFile="Data/simple-object.json")
# Uncomment the following to generate the expected results.
# WriteObjectToJSON(ObjectID="TestObject",OutputFile="ExpectedResults/test-NewObject-json-root-object-out.json")
WriteObjectToJSON(ObjectID="TestObject",OutputFile="Results/test-NewObject-json-root-object-out.json")
# Comparison the results with expected.
CompareFiles(InputFile1="ExpectedResults/test-NewObject-json-root-object-out.json",InputFile2="Results/test-NewObject-json-root-object-out.json",IfDifferent=Warn)
