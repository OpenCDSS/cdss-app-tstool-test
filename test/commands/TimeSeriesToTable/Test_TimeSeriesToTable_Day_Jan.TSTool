# Test copying daily time series to a table, and also create the table
# Only copy January data values
StartLog(LogFile="Results/Test_TimeSeriesToTable_Day_Jan.TSTool.log")
RemoveFile(InputFile="Results/Test_TimeSeriesToTable_Day_Jan_out.csv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1..Flow.Day",SetStart="2000-01-01",SetEnd="2003-12-12",Units="ACFT",PatternValues="1,-2,3,-4,5,,7,-8,9,-10,11,-12")
NewPatternTimeSeries(Alias="ts2",NewTSID="ts2..Flow.Day",SetStart="2001-03-01",SetEnd="2004-05-20",Units="ACFT",PatternValues="-1,2,-3,4,-5,,-7,8,-9,10,-11,12")
TimeSeriesToTable(TableID="TestTable",DateTimeColumn="Year",ValueColumn="%L-%T",DataRow=1,OutputWindowStart="01-01",OutputWindowEnd="01-31",IfTableNotFound="Create")
# Uncomment the following to recreate expected results
# WriteTableToDelimitedFile(TableID="TestTable",OutputFile="ExpectedResults\Test_TimeSeriesToTable_Day_Jan_out.csv")
# Generate the results.
WriteTableToDelimitedFile(TableID="TestTable",OutputFile="Results\Test_TimeSeriesToTable_Day_Jan_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_TimeSeriesToTable_Day_Jan_out.csv",InputFile2="Results/Test_TimeSeriesToTable_Day_Jan_out.csv",IfDifferent=Warn)
