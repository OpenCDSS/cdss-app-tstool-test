NewPatternTimeSeries(Alias="loc1",NewTSID="loc1..QIN.6Hour",Description="Test data",SetStart="2007-11-01 00 PST",SetEnd="2007-11-15 18 PST",Units="CFS",PatternValues="5,10,12,13,75")
