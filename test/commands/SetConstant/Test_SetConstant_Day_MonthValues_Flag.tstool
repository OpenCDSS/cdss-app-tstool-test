# Test setting time series constant using monthly values
# - daily time series
# - monthly values, some missing, some left
# - Also set the data flag on one time series
StartLog(LogFile="Results/Test_SetConstant_Day_MonthValues_Flag.tstool.log")
RemoveFile(InputFile="Results/Test_SetConstant_Day_MonthValues_Flag_out.dv",IfNotFound="Ignore")
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",MissingValue="NaN",PatternValues="1,2,3,2,1,,5,1,,,,1,3,5")
NewPatternTimeSeries(Alias="ts2",NewTSID="ts2...Day",Description="test data 2",SetStart="2002-01-01",SetEnd="2003-07-28",MissingValue="NaN",PatternValues="1,2,3,2,1,,5,1,,,,1,3,5")
SetConstant(TSList="AllMatchingTSID",TSID="ts1",ConstantValue="2.55",SetStart="2000-03-20",SetEnd="2001-07-02")
SetConstant(TSList="AllMatchingTSID",TSID="ts2",MonthValues="*,*,*,4,5,6,,,9,10,NaN,12",SetFlag="X",SetFlagDescription="X flag")
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/Test_SetConstant_Day_MonthValues_Flag_out.dv")
WriteDateValue(OutputFile="Results/Test_SetConstant_Day_MonthValues_Flag_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_SetConstant_Day_MonthValues_Flag_out.dv",InputFile2="Results/Test_SetConstant_Day_MonthValues_Flag_out.dv",IfDifferent="Warn")
