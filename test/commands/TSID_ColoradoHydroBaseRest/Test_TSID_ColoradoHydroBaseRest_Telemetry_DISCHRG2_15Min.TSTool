CompareFiles(InputFile1="ExpectedResults/Test_TSID_ColoradoHydroBaseRest_Telemetry_DISCHRG2_15Min_out.dv",InputFile2="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_DISCHRG2_15Min_out.dv",IfDifferent=Warn)
