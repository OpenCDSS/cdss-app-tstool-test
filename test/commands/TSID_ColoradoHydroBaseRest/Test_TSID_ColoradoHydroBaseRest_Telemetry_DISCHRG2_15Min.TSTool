# Test reading an individual Telemetry DISCHRG2 15 min time series from ColoradoHydroBaseRest web service using a TSID
StartLog(LogFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_DISCHRG2_15Min.TSTool.log")
RemoveFile(InputFile="Results/Test_TSID_ColoradoWaterHydroBaseRest_Telemetry_DISCHRG2_15Min_out.dv",IfNotFound=Ignore)
