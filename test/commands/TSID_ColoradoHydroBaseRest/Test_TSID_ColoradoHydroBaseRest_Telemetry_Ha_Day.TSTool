# Test reading an individual Telemetry Ha Day time series from ColoradoHydroBaseRest web service using a TSID
StartLog(LogFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_Ha_Day.TSTool.log")
RemoveFile(InputFile="Results/Test_TSID_ColoradoWaterHydroBaseRest_Telemetry_Ha_Day_out.dv",IfNotFound="Ignore")
# Read a short period and visually compare to web service results
SetInputPeriod(InputStart="2018-01-01",InputEnd="2018-03-15")
abbrev:RFHCANCO.DWR.Ha.Day~HydroBaseWeb
WriteDateValue(OutputFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_Ha_Day_out.dv",Precision="2")
# Uncomment the following line to regenerate expected results
# CopyFile(InputFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_Ha_Day_out.dv",OutputFile="ExpectedResults/Test_TSID_ColoradoHydroBaseRest_Telemetry_Ha_Day_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_TSID_ColoradoHydroBaseRest_Telemetry_Ha_Day_out.dv",InputFile2="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_Ha_Day_out.dv",IfDifferent="Warn")
