StartLog(LogFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_DISCHRG3_Hour.TSTool.log")
RemoveFile(InputFile="Results/Test_TSID_ColoradoWaterHydroBaseRest_Telemetry_DISCHRG3_Hour_out.dv",IfNotFound="Ignore")
