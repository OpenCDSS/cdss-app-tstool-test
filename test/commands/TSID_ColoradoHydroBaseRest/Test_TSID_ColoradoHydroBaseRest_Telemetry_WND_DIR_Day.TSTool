CompareFiles(InputFile1="ExpectedResults/Test_TSID_ColoradoHydroBaseRest_Telemetry_WND_DIR_Day_out.dv",InputFile2="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_WND_DIR_Day_out.dv",IfDifferent=Warn)
