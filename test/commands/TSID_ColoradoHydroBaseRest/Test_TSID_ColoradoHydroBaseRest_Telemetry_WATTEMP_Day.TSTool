# Test reading an individual Telemetry WATTEMP Day time series from ColoradoHydroBaseRest web service using a TSID
StartLog(LogFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_WATTEMP_Day.TSTool.log")
RemoveFile(InputFile="Results/Test_TSID_ColoradoWaterHydroBaseRest_Telemetry_WATTEMP_Day_out.dv",IfNotFound=Ignore)
# Read a short period and visually compare with web services results
SetInputPeriod(InputStart="2018-01-01",InputEnd="2018-03-15")
abbrev:COMRETCO.DWR.WATTEMP.Day~HydroBaseWeb
# Uncomment the following line to regenerate expected results
# WriteDateValue(OutputFile="ExpectedResults/Test_TSID_ColoradoHydroBaseRest_Telemetry_WATTEMP_Day_out.dv",Precision=2)
WriteDateValue(OutputFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_WATTEMP_Day_out.dv",Precision=2)
CompareFiles(InputFile1="ExpectedResults/Test_TSID_ColoradoHydroBaseRest_Telemetry_WATTEMP_Day_out.dv",InputFile2="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_WATTEMP_Day_out.dv",IfDifferent=Warn)
