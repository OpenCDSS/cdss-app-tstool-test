SetInputPeriod(InputStart="2018-01-01 00",InputEnd="2018-01-03 12")
abbrev:TWIRESCO.BOR.WND_DIR.Hour~HydroBaseWeb
WriteDateValue(OutputFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_WND_DIR_Hour_out.dv",Precision="2")
