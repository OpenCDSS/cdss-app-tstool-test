# Test reading an individual Telemetry WIND_DIR Hour time series from ColoradoHydroBaseRest web service using a TSID
StartLog(LogFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_WND_DIR_Hour.TSTool.log")
RemoveFile(InputFile="Results/Test_TSID_ColoradoWaterHydroBaseRest_Telemetry_WND_DIR_Hour_out.dv",IfNotFound="Ignore")
