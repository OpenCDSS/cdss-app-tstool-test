WritePropertiesToFile(OutputFile="Results/Test_If_TSHasData.txt",IncludeProperties="AliasExists,MissingAliasExists,AliasDoesNotExist,MissingAliasDoesNotExist,TSIDExists,MissingTSIDExists,TSIDDoesNotExist,MissingTSIDDoesNotExist,ts1HasData,ts1HasNoData,ts2HasData,ts2HasNoData")
CompareFiles(InputFile1="ExpectedResults/Test_If_TSHasData.txt",InputFile2="Results/Test_If_TSHasData.txt",IfDifferent="Warn")
