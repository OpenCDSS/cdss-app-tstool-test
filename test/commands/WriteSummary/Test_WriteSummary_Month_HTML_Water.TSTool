WriteSummary(OutputFile="Results/Test_WriteSummary_Month_HTML_Water_out.html",OutputYearType=Water)
# Do regression comparison - allow 4 diffs for date/version/commmand line/machine
CompareFiles(InputFile1="ExpectedResults\Test_WriteSummary_Month_HTML_Water_out.html",InputFile2="Results/Test_WriteSummary_Month_HTML_Water_out.html",AllowedDiff="4",IfDifferent=Warn)
