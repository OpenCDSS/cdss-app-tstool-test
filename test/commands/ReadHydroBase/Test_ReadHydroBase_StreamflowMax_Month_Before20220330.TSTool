# Test reading a StreamflowMax time series for specific structure
# - station 06754000
# - monthly data
#@require datastore HydroBase version < 20220330
StartLog(LogFile="Results/Test_ReadHydroBase_StreamflowMax_Month_Before20220330.TSTool.log")
# Set the input period to bound missing values.
ReadHydroBase(DataType="Streamflow",Interval="Month",Where1="Station ID;Matches;06754000",InputStart="1912-01",InputEnd="1914-12")
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults/Test_ReadHydroBase_StreamflowMax_Month_Before20220330_out.dv")
WriteDateValue(OutputFile="Results/Test_ReadHydroBase_StreamflowMax_Month_Before20220330_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_ReadHydroBase_StreamflowMax_Month_Before20220330_out.dv",InputFile2="Results/Test_ReadHydroBase_StreamflowMax_Month_Before20220330_out.dv",IfDifferent=Warn)
