# Test reading a table from Microsoft Access:
# - copy the blank test datbase, open as a datstore, populate some data, and read
# - consequently, this test uses several commands that must all work for Access
StartLog(LogFile="Results/Test_ReadTableFromDataStore_Access.TSTool.log")
RemoveFile(InputFile="Results/Test_ReadTableFromDataStore_Access_out.csv",IfNotFound=Ignore)
# Open a SQLite database.
OpenDataStore(DataStoreName="TestSQLiteDatastore",DataStoreDescription="Test SQLite datastore",DataStoreType="GenericDatabaseDataStore",DatabaseEngine="SQLite",ServerName="Data/test-sqlite.db")
ReadTableFromDataStore(DataStore="TestSQLiteDatastore",Sql="SELECT * FROM table1 WHERE name = 'name1'",OutputProperties="name:TestProperty_name,integer1:TestProperty_integer1,float1:TestProperty_float1",TableID="TestTable")
CloseDataStore(DataStore="TestSQLiteDatastore",StatusMessage="Closed after test complete")
# Uncomment the following 2 commands to regenerate expected results.
# WriteTableToDelimitedFile(TableID="TestTable",OutputFile="ExpectedResults/Test_ReadTableFromDataStore_Access_out.csv")
# WritePropertiesToFile(OutputFile="ExpectedResults/Test_ReadTableFromDataStore_Access_out.txt",IncludeProperties="TestProperty_name,TestProperty_integer1,TestProperty_float1",FileFormat=NameValue)
WriteTableToDelimitedFile(TableID="TestTable",OutputFile="Results/Test_ReadTableFromDataStore_Access_out.csv")
WritePropertiesToFile(OutputFile="Results/Test_ReadTableFromDataStore_Access_out.txt",IncludeProperties="TestProperty_name,TestProperty_integer1,TestProperty_float1",FileFormat=NameValue)
# Do regression comparison with the expected results for the properties.
CompareFiles(InputFile1="ExpectedResults/Test_ReadTableFromDataStore_Access_out.csv",InputFile2="Results/Test_ReadTableFromDataStore_Access_out.csv",IfDifferent=Warn)
CompareFiles(InputFile1="ExpectedResults/Test_ReadTableFromDataStore_Access_out.txt",InputFile2="Results/Test_ReadTableFromDataStore_Access_out.txt",IfDifferent=Warn)
