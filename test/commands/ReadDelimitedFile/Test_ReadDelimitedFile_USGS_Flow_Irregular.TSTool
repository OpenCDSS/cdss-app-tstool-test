# Test reading a USGS tab-delimited flow file downloaded from the web,
# read as irregular interval
StartLog(LogFile="Results/Test_ReadDelimitedFile_USGS_Flow_Irregular.TSTool.log")
RemoveFile(InputFile="Results\Test_ReadDelimitedFile_USGS_Flow_Irregular_out.dv",IfNotFound=Ignore)
ReadDelimitedFile(InputFile="Data\USGS-G03451500.txt",Delimiter="\t",ColumnNames="Agency,ID,Date,Flow",DateTimeColumn="Date",ValueColumn="Flow",Comment="#<",SkipRowsAfterComments="2",LocationID="03451500",Provider="USGS",Interval=IRREGULAR,Units="cfs",Alias="%L%T")
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults\Test_ReadDelimitedFile_USGS_Flow_Irregular_out.dv",Precision=2)
WriteDateValue(OutputFile="Results\Test_ReadDelimitedFile_USGS_Flow_Irregular_out.dv",Precision=2)
CompareFiles(InputFile1="ExpectedResults\Test_ReadDelimitedFile_USGS_Flow_Irregular_out.dv",InputFile2="Results\Test_ReadDelimitedFile_USGS_Flow_Irregular_out.dv",IfDifferent=Warn)
