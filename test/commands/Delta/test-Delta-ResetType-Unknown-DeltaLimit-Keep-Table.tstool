StartLog(LogFile="Results/test-Delta-ResetType-Unknown-DeltaLimit-Keep-Table.tstool.log")
# Test computing the delta:
# - ResetType=Unknown (default for simple delta, so no reset)
# - variable data
# - no reset
# - check the DeltaLimit value
# - keep the calculated values even though out of range
# - issues are expected and output to a table
RemoveFile(InputFile="Results/test-Delta-ResetType-Unknown-DeltaLimit-Keep-Table_out.dv",IfNotFound="Ignore")
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",PatternValues="1,2,3,4,7,,10,,,,11,13,15")
NewPatternTimeSeries(Alias="ts2",NewTSID="ts2...Day",Description="test data 2",SetStart="2002-01-01",SetEnd="2003-07-28",PatternValues="2,3,4,5,8,15,21,,,,31,33,35")
# SetDebugLevel(LogFileLevel="1")
Delta(Alias="%L-delta",DeltaLimit="2.5",DeltaLimitAction="Keep",DeltaLimitFlag="X",TableID="Test_Delta_Table",TableTSIDColumn="TSID",TableTSIDFormat="%L")
# SetDebugLevel(ScreenLevel="0",LogFileLevel="0")
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/test-Delta-ResetType-Unknown-DeltaLimit-Keep-Table-out.dv")
WriteDateValue(OutputFile="Results/test-Delta-ResetType-Unknown-DeltaLimit-Keep-Table-out.dv")
CompareFiles(InputFile1="ExpectedResults/test-Delta-ResetType-Unknown-DeltaLimit-Keep-Table-out.dv",InputFile2="Results/test-Delta-ResetType-Unknown-DeltaLimit-Keep-Table-out.dv",IfDifferent="Warn")
