# Test adding time series
# - specify the analyis period
StartLog(LogFile="Results/Test_Add_Day_AnalysisPeriod.TSTool.log")
RemoveFile(InputFile="Results/Test_Add_Day_AnalysisPeriod_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",PatternValues="1")
NewPatternTimeSeries(Alias="ts2",NewTSID="ts2...Day",Description="test data 2",SetStart="2002-01-01",SetEnd="2003-07-28",PatternValues="2")
Add(TSID="ts1",AddTSList=SpecifiedTSID,AddTSID="ts2",HandleMissingHow="IgnoreMissing",AnalysisStart="2002-03-01",AnalysisEnd="2002-12-31")
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/Test_Add_Day_AnalysisPeriod_out.dv")
WriteDateValue(OutputFile="Results/Test_Add_Day_AnalysisPeriod_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_Add_Day_AnalysisPeriod_out.dv",InputFile2="Results/Test_Add_Day_AnalysisPeriod_out.dv",IfDifferent=Warn)
