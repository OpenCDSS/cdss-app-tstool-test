# For daily data, determine the DayOfFirstLT value in each year, specifying SearchStart.
# The expected daily value is 182
StartLog(LogFile="Results/Test_NewStatisticYearTS_Day_DayOfFirstLT_SearchStart.TSTool.log")
RemoveFile(InputFile="Results\Test_NewStatisticYearTS_Day_DayOfFirstLT_SearchStart_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1..Streamflow.Day",Description="Test data",SetStart="1950-01-01",SetEnd="1951-03-12",Units="CFS",PatternValues="5,10,12,13,75,,50,45,20,4")
NewStatisticYearTS(Alias="Stat",TSID="ts1",NewTSID="ts1..Streamflow.Year.DayOfFirstLT",Statistic=DayOfFirstLT,TestValue=50,SearchStart="07-01")
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults\Test_NewStatisticYearTS_Day_DayOfFirstLT_SearchStart_out.dv",TSList=AllMatchingTSID,TSID="Stat")
WriteDateValue(OutputFile="Results\Test_NewStatisticYearTS_Day_DayOfFirstLT_SearchStart_out.dv",TSList=AllMatchingTSID,TSID="Stat")
CompareFiles(InputFile1="ExpectedResults\Test_NewStatisticYearTS_Day_DayOfFirstLT_SearchStart_out.dv",InputFile2="Results\Test_NewStatisticYearTS_Day_DayOfFirstLT_SearchStart_out.dv",IfDifferent=Warn)
