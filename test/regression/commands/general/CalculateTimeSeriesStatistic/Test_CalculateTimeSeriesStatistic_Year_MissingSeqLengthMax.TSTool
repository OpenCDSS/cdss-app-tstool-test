# Calculate the MissingSeqLengthMax statistic for time series and save to a table, using Year interval data
# Biggest gap is 3 missing
StartLog(LogFile="Results/Test_CalculateTimeSeriesStatistic_Year_MissingSeqLengthMax.TSTool.log")
RemoveFile(InputFile="Results/Test_CalculateTimeSeriesStatistic_Year_MissingSeqLengthMax_out.csv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1...Year",Description="test data 1",SetStart="1970",SetEnd="1994",Units="ACFT",PatternValues="9,8,9,10,,11,12,11,10,7,8,10,13,,,12,10,5,6,10,15,14,,,,,10,9,10,11,10,10")
NewPatternTimeSeries(Alias="ts2",NewTSID="ts2...Year",Description="test data 2",SetStart="1970",SetEnd="1994",Units="ACFT",PatternValues="18,16,,18,20,22,24,22,20,14,16,20,26,24,,20,10,12,20,30,28,20,18,20,,,,22,20,20")
NewTable(TableID="Table1",Columns="TSID,string;MissingSeqLengthMax,double")
CalculateTimeSeriesStatistic(Statistic="MissingSeqLengthMax",TableID="Table1",TableTSIDColumn="TSID",TableStatisticColumn="MissingSeqLengthMax")
# Uncomment the following command to regenerate expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults/Test_CalculateTimeSeriesStatistic_Year_MissingSeqLengthMax_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results/Test_CalculateTimeSeriesStatistic_Year_MissingSeqLengthMax_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_CalculateTimeSeriesStatistic_Year_MissingSeqLengthMax_out.csv",InputFile2="Results/Test_CalculateTimeSeriesStatistic_Year_MissingSeqLengthMax_out.csv",IfDifferent=Warn)
