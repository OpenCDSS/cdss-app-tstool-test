# Calculate the Mean, Max, and Min statistics for time series and save to a table
# This checks matching the TSID to put the results in one row
StartLog(LogFile="Results/Test_CalculateTimeSeriesStatistic_MeanMaxMin.TSTool.log")
RemoveFile(InputFile="Results/Test_CalculateTimeSeriesStatistic_MeanMaxMin_out.dv",IfNotFound=Ignore)
TS ts1 = NewPatternTimeSeries(NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",PatternValues="1,2,3,2,1,-999,5,1,-999,-999,-999,1,3,5")
TS ts2 = NewPatternTimeSeries(NewTSID="ts2...Day",Description="test data 2",SetStart="2002-01-01",SetEnd="2003-07-28",PatternValues="11,22,33,22,11,-999,55,11,-999,-999,-999,11,33,55")
NewTable(TableID="Table1",Columns="TSID,string")
CalculateTimeSeriesStatistic(Statistic="Mean",TableID="Table1",TableTSIDColumn="TSID",TableStatisticColumn="Mean")
CalculateTimeSeriesStatistic(Statistic="Max",TableID="Table1",TableTSIDColumn="TSID",TableStatisticColumn="Max")
CalculateTimeSeriesStatistic(Statistic="Min",TableID="Table1",TableTSIDColumn="TSID",TableStatisticColumn="Min")
# Uncomment the following command to regenerate expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults/Test_CalculateTimeSeriesStatistic_MeanMaxMin_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results/Test_CalculateTimeSeriesStatistic_MeanMaxMin_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_CalculateTimeSeriesStatistic_MeanMaxMin_out.csv",InputFile2="Results/Test_CalculateTimeSeriesStatistic_MeanMaxMin_out.csv",IfDifferent=Warn)
