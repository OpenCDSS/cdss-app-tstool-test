# Calculate the Last statistic for time series and save to a table
# - specify analysis period so last value should be at the end of the period
# - specify the analysis window so last value should be end of the window in analysis period
StartLog(LogFile="Results/Test_CalculateTimeSeriesStatistic_Last_AnalysisPeriod_AnalysisWindow.TSTool.log")
RemoveFile(InputFile="Results/Test_CalculateTimeSeriesStatistic_Last_AnalysisPeriod_AnalysisWindow_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",PatternValues="1,2,3,2,1,-999,5,1,-999,-999,-999,1,3,5")
NewPatternTimeSeries(Alias="ts2",NewTSID="ts2...Day",Description="test data 2",SetStart="2002-01-01",SetEnd="2003-07-28",PatternValues="11,22,33,22,11,-999,55,11,-999,-999,-999,11,33,55")
NewTable(TableID="Table1",Columns="TSID,string;Last,double")
CalculateTimeSeriesStatistic(Statistic="Last",AnalysisWindowStart="01-01",AnalysisWindowEnd="03-07",TableID="Table1",TableTSIDColumn="TSID",TableStatisticColumn="Last")
# Uncomment the following command to regenerate expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults/Test_CalculateTimeSeriesStatistic_Last_AnalysisPeriod_AnalysisWindow_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results/Test_CalculateTimeSeriesStatistic_Last_AnalysisPeriod_AnalysisWindow_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_CalculateTimeSeriesStatistic_Last_AnalysisPeriod_AnalysisWindow_out.csv",InputFile2="Results/Test_CalculateTimeSeriesStatistic_Last_AnalysisPeriod_AnalysisWindow_out.csv",IfDifferent=Warn)
