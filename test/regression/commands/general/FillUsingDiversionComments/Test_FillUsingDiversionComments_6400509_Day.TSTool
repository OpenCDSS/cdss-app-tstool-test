# Test filling HydroBase diversion records using diversion comments and CIU flag.
# - use a specified period to limit HydroBase update impacts on the test.
# - use a structure that has known data so that combinations can be validated
# - daily data
# @testSuite HydroBase
StartLog(LogFile="Results/Test_FillUsingDiversionComments_6400509_Day.TSTool.log")
RemoveFile(InputFile="Results/Test_FillUsingDiversionComments_6400509_Day_out.dv",IfNotFound=Ignore)
SetInputPeriod(InputStart="11/30/1949",InputEnd="10/31/2013")
SetOutputPeriod(OutputStart="11/30/1949",OutputEnd="10/31/2013")
# 6400509 - TAMARACK DITCH
# Fill only with diversion comments
ReadTimeSeries(TSID="6400509.DWR.DivTotal.Day~HydroBase",Alias="%L-DCOnly",IfNotFound=Warn)
FillUsingDiversionComments(TSList=AllMatchingTSID,TSID="6400509-DCOnly",FillFlag="Auto",FillFlagDescription="Auto",RecalcLimits=True,FillUsingCIU=False)
# Fill with diversion comments and CIU
ReadTimeSeries(TSID="6400509.DWR.DivTotal.Day~HydroBase",Alias="%L-DCAndCIU",IfNotFound=Warn)
FillUsingDiversionComments(TSList=AllMatchingTSID,TSID="6400509-DCAndCIU",FillFlag="Auto",FillFlagDescription="Auto",RecalcLimits=True,FillUsingCIU=True,FillUsingCIUFlag="Auto",FillUsingCIUFlagDescription="Auto")
# Read again without any filling
ReadTimeSeries(TSID="6400509.DWR.DivTotal.Day~HydroBase",Alias="%L-Raw",IfNotFound=Warn)
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/Test_FillUsingDiversionComments_6400509_Day_out.dv")
WriteDateValue(OutputFile="Results/Test_FillUsingDiversionComments_6400509_Day_out.dv")
# Also write an Excel file to simplify review - only uncomment for troubleshooting because it is slow
# WriteTimeSeriesToExcel(MissingValue=Blank,OutputFile="Results/Test_FillUsingDiversionComments_6400509_Day_out.xlsx",Worksheet="Daily Data",ExcelAddress="A1",ValueColumns="%A",Author="TSTool",ColumnComment="%A (%U)",ValueComment="Flag: ${tsdata:flag}")
CompareFiles(InputFile1="ExpectedResults/Test_FillUsingDiversionComments_6400509_Day_out.dv",InputFile2="Results/Test_FillUsingDiversionComments_6400509_Day_out.dv",IfDifferent=Warn)
