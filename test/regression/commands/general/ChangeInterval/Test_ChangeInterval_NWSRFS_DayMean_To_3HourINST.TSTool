# Test change interval from mean daily flow to 3-hour flow, and compare against NWSRFS mcp3 output
StartLog(LogFile="Results\Test_ChangeInterval_DayMean_To_3HourINST.TSTool.log")
RemoveFile(InputFile="Results\Test_ChangeInterval_DayMean_To_3HourINST_out.dv",IfNotFound=Ignore)
#
# Read NWSCard file generated by the Test_ChangeInterval_DayMean_To_6HourINST.TSTool commands file
TS DayMEAN = ReadNwsCard(InputFile="Data\CHANGE-T_DAY_IN.SQME",Read24HourAsDay=True)
#
TS 3HourINST = ChangeInterval(TSID="DayMEAN",NewInterval=3Hour,OldTimeScale=MEAN,NewTimeScale=INST,NewDataType=CMS)
#
# Read the output from mcp3 CHANGE-T command
TS mcp3_3hr = ReadNwsCard(InputFile="Data\CHANGE-T_3hr_OUT.SQIN")
CompareTimeSeries(MatchDataType=True,Precision=5,Tolerance="0.00002",WarnIfDifferent=True)


