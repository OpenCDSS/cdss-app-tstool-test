# Test changing ACCM to MEAN, small to large interval
#
StartLog(LogFile="Results\Test_ChangeInterval_Reg-Reg_ACCM-MEAN-SmallToLarge.TSTool.log")
RemoveFile(InputFile="Results\Test_ChangeInterval_Reg-Reg_ACCM-MEAN-SmallToLarge.dv",IfNotFound=Ignore)
#
TS Original = NewPatternTimeSeries(NewTSID="ts1..Flow.6Hour",Description="Test data",SetStart="2006-12-15 00",SetEnd="2007-01-29 00",Units="CFS",PatternValues="25,55,19,32,20,15,35,60,30,40,-999,-999,25,70")
TS Changed_noOptions = ChangeInterval(TSID="Original",NewInterval=Day,OldTimeScale=ACCM,NewTimeScale=MEAN)
TS Changed_Endpoints-AvgEndpoints = ChangeInterval(TSID="Original",NewInterval=Day,OldTimeScale=ACCM,NewTimeScale=MEAN,HandleEndpointsHow=AverageEndpoints)
TS Changed_Endpoints-IncludeFirstOnly = ChangeInterval(TSID="Original",NewInterval=Day,OldTimeScale=ACCM,NewTimeScale=MEAN,HandleEndpointsHow=IncludeFirstOnly)
Free(TSList=AllMatchingTSID,TSID="Original")
#
# Uncomment the following command to regenerate the expected results file.
WriteDateValue(OutputFile="ExpectedResults\Test_ChangeInterval_Reg-Reg_ACCM-MEAN-SmallToLarge.dv",TSList=AllMatchingTSID,TSID="*")
WriteDateValue(OutputFile="Results\Test_ChangeInterval_Reg-Reg_ACCM-MEAN-SmallToLarge.dv",TSList=AllMatchingTSID,TSID="*")
#
CompareFiles(InputFile1="ExpectedResults\Test_ChangeInterval_Reg-Reg_ACCM-MEAN-SmallToLarge.dv",InputFile2="Results\Test_ChangeInterval_Reg-Reg_ACCM-MEAN-SmallToLarge.dv",WarnIfDifferent=True)
