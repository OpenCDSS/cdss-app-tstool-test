# Test changing MEAN to MEAN, large to small interval
#
StartLog(LogFile="Results\Test_ChangeInterval_Reg-Reg_MEAN-MEAN-LargeToSmall.TSTool.log")
RemoveFile(InputFile="Results\Test_ChangeInterval_Reg-Reg_MEAN-MEAN-LargeToSmall.dv",IfNotFound=Ignore)
#
NewPatternTimeSeries(Alias="Original",NewTSID="ts1..Flow.6Hour",Description="Test data",SetStart="2006-12-15 00",SetEnd="2007-01-29 00",Units="CFS",PatternValues="25,55,19,32,20,15,35,60,30,40,-999,-999,25,70")
ChangeInterval(Alias="Changed_noOptions",TSID="Original",NewInterval=2Hour,OldTimeScale=MEAN,NewTimeScale=MEAN)
ChangeInterval(Alias="Changed_OutputFill-Interpolate",TSID="Original",NewInterval=2Hour,OldTimeScale=MEAN,NewTimeScale=MEAN,OutputFillMethod=Interpolate)
ChangeInterval(Alias="Changed_OutputFill-Repeat",TSID="Original",NewInterval=2Hour,OldTimeScale=MEAN,NewTimeScale=MEAN,OutputFillMethod=Repeat)
Free(TSList=AllMatchingTSID,TSID="Original")
#
# Uncomment the following command to regenerate the expected results file.
#WriteDateValue(OutputFile="ExpectedResults\Test_ChangeInterval_Reg-Reg_MEAN-MEAN-LargeToSmall.dv",TSList=AllMatchingTSID,TSID="*")
WriteDateValue(OutputFile="Results\Test_ChangeInterval_Reg-Reg_MEAN-MEAN-LargeToSmall.dv",TSList=AllMatchingTSID,TSID="*")
#
CompareFiles(InputFile1="ExpectedResults\Test_ChangeInterval_Reg-Reg_MEAN-MEAN-LargeToSmall.dv",InputFile2="Results\Test_ChangeInterval_Reg-Reg_MEAN-MEAN-LargeToSmall.dv",IfDifferent=Warn)
