# Test changing monthly ACCM to water year MEAN, with period that aligns with
# water years.
#
StartLog(LogFile="Results\Test_ChangeInterval_Month-ACCM_Year-MEAN_WaterYear.TSTool.log")
RemoveFile(InputFile="Results\Test_ChangeInterval_Month-ACCM_Year-MEAN_WaterYear.dv",IfNotFound=Ignore)
# Test data accumulate to 120 in years with full data (mean 10),
# 110 in years with 1 missing value
TS Original = NewPatternTimeSeries(NewTSID="ts1..Flow.Month",Description="Test data",SetStart="2000-10",SetEnd="2004-09",Units="CFS",PatternValues="5,10,15,,10,15,5,10,10,5,10,15,5,10,15,10,10,15,5,10,10,5,10,15")
TS New = ChangeInterval(TSID="Original",NewInterval=Year,OldTimeScale=ACCM,NewTimeScale=MEAN,OutputYearType=Water)
#
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults\Test_ChangeInterval_Month-ACCM_Year-MEAN_WaterYear.dv",TSList=AllMatchingTSID,TSID="New")
WriteDateValue(OutputFile="Results\Test_ChangeInterval_Month-ACCM_Year-MEAN_WaterYear.dv",TSList=AllMatchingTSID,TSID="New")
#
CompareFiles(InputFile1="ExpectedResults\Test_ChangeInterval_Month-ACCM_Year-MEAN_WaterYear.dv",InputFile2="Results\Test_ChangeInterval_Month-ACCM_Year-MEAN_WaterYear.dv",IfDifferent=Warn)
