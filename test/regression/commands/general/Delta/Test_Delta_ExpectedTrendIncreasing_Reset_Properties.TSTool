# Test computing the delta with trend increasing and reset min/max specified
# - Expect to have warnings because some values are out of range
# - Specify parameters with ${Property}
# @expectedStatus Warning
StartLog(LogFile="Results/Test_Delta_ExpectedTrendIncreasing_Reset_Properties.TSTool.log")
RemoveFile(InputFile="Results/Test_Delta_ExpectedTrendIncreasing_Reset_Properties_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",PatternValues="1,2,3,4,7,,10,,,,11,13,15")
NewPatternTimeSeries(Alias="ts2",NewTSID="ts2...Day",Description="test data 2",SetStart="2002-01-01",SetEnd="2003-07-28",PatternValues="2,3,4,5,8,15,21,,,,31,33,35")
SetProperty(PropertyName="TestTSID",PropertyType=String,PropertyValue="*")
SetProperty(PropertyName="TestAnalysisStart",PropertyType=DateTime,PropertyValue="2000-01-01")
SetProperty(PropertyName="TestAnalysisEnd",PropertyType=DateTime,PropertyValue="2003-07-01")
SetProperty(PropertyName="TestFlag",PropertyType=String,PropertyValue="x")
Delta(TSList=AllMatchingTSID,TSID="${TestTSID}",ExpectedTrend=Increasing,ResetMin=0,ResetMax=20,AnalysisStart="${TestAnalysisStart}",AnalysisEnd="${TestAnalysisEnd}",Flag="${TestFlag}",Alias="%L-delta")
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/Test_Delta_ExpectedTrendIncreasing_Reset_Properties_out.dv")
WriteDateValue(OutputFile="Results/Test_Delta_ExpectedTrendIncreasing_Reset_Properties_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_Delta_ExpectedTrendIncreasing_Reset_Properties_out.dv",InputFile2="Results/Test_Delta_ExpectedTrendIncreasing_Reset_Properties_out.dv",IfDifferent=Warn)
