# Test creating end of month time series from daily data
# -specify the daily time series using a ${Property}
StartLog(LogFile="Results/Test_NewEndOfMonthTSFromDayTS_Bracket=15_TSIDProperty.TSTool.log")
RemoveFile(InputFile="Results\Test_NewEndOfMonthTSFromDayTS_Bracket=15_TSIDProperty_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1_day",NewTSID="ts1..MyData.Day",Description="Test data",SetStart="1950-01-01",SetEnd="1951-03-12",Units="CFS",PatternValues="5,10,12,13,75,50,25,7,5")
SetOutputPeriod(OutputStart="1950-01-01",OutputEnd="1951-03-31")
SetProperty(PropertyName="id",PropertyType=String,PropertyValue="ts1")
NewEndOfMonthTSFromDayTS(DayTSID="${id}_day",Alias="tsout",Bracket=15)
SelectTimeSeries(TSList=AllMatchingTSID,TSID="tsout",DeselectAllFirst=True)
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(TSList=SelectedTS,OutputFile="ExpectedResults\Test_NewEndOfMonthTSFromDayTS_Bracket=15_TSIDProperty_out.dv")
WriteDateValue(TSList=SelectedTS,OutputFile="Results\Test_NewEndOfMonthTSFromDayTS_Bracket=15_TSIDProperty_out.dv")
CompareFiles(InputFile1="ExpectedResults\Test_NewEndOfMonthTSFromDayTS_Bracket=15_TSIDProperty_out.dv",InputFile2="Results\Test_NewEndOfMonthTSFromDayTS_Bracket=15_TSIDProperty_out.dv",IfDifferent=Warn)
