WriteRiversideDB(DataStore="RiversideDB_TSTool",Location="AS_1_4",DataSource="UFI",DataType="CHL",DataSubType="1",Interval="IRREGULAR",Scenario="RAW")
CompareFiles(InputFile1="ExpectedResults/Test_WriteTimeSeries_15Minute_out.dv",InputFile2="Results/Test_WriteTimeSeries_15Minute_out.dv",IfDifferent=Warn)
