# Test reading a irregular day interval time series from a DateValue file
StartLog(LogFile="Results/Test_ReadDateValue_IrregularDay.TSTool.log")
RemoveFile(InputFile="Results\Test_ReadDateValue_IrregularDay_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="MyLoc",NewTSID="MyLoc..MyData.Irregular",IrregularInterval=Day,Description="Test data",SetStart="1950-01-01",SetEnd="1951-03-12",Units="CFS",PatternValues="5,10,12,13,75",PatternFlags="aa,b,,dd,e")
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults\Test_ReadDateValue_IrregularDay_out.dv")
Free(TSList=AllMatchingTSID,TSID="*")
ReadDateValue(InputFile="ExpectedResults\Test_ReadDateValue_IrregularDay_out.dv")
WriteDateValue(OutputFile="Results\Test_ReadDateValue_IrregularDay_out.dv")
CompareFiles(InputFile1="ExpectedResults\Test_ReadDateValue_IrregularDay_out.dv",InputFile2="Results\Test_ReadDateValue_IrregularDay_out.dv",IfDifferent=Warn)
