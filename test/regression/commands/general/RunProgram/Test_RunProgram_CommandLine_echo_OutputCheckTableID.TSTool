# Test running an external program using a full command line with other defaults
# - use a command shell internally to run and determine the exit status from the process exit value.
# - output to a file and check output using table of patterns
StartLog(LogFile="Results/Test_RunProgram_CommandLine_echo_OutputCheckTableID.TSTool.log")
RemoveFile(InputFile="Results\Test_RunProgram_CommandLine_echo_OutputCheckTableID_out.txt",IfNotFound=Ignore)
ReadTableFromDelimitedFile(TableID="OutputChecksTable",InputFile="Data\output-checks1.csv")
# Uncomment the following command to regenerate the expected results file.
# RunProgram(CommandLine="echo Hello > ${WorkingDir}/ExpectedResults/Test_RunProgram_CommandLine_echo_OutputCheckTableID_out.txt")
# Generate the output
RunProgram(CommandLine="echo Hello > ${WorkingDir}/Results/Test_RunProgram_CommandLine_echo_OutputCheckTableID_out.txt")
CompareFiles(InputFile1="ExpectedResults/Test_RunProgram_CommandLine_echo_OutputCheckTableID_out.txt",InputFile2="Results/Test_RunProgram_CommandLine_echo_OutputCheckTableID_out.txt",IfDifferent=Warn)
