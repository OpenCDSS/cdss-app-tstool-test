# Test running an external program using a full command line with other defaults
# This will use a command shell internally to run and determine the exit status from
# the process exit value.
StartLog(LogFile="Results/Test_RunProgram_CommandLine_echo.TSTool.log")
RemoveFile(InputFile="Results\Test_RunProgram_CommandLine_echo_out.txt",IfNotFound=Ignore)
# Uncomment the following command to regenerate the expected results file.
# RunProgram(CommandLine="echo Hello > ${WorkingDir}/ExpectedResults/Test_RunProgram_CommandLine_echo_out.txt")
# Generate the output
RunProgram(CommandLine="echo Hello > ${WorkingDir}/Results/Test_RunProgram_CommandLine_echo_out.txt")
CompareFiles(InputFile1="ExpectedResults/Test_RunProgram_CommandLine_echo_out.txt",InputFile2="Results/Test_RunProgram_CommandLine_echo_out.txt",IfDifferent=Warn)
