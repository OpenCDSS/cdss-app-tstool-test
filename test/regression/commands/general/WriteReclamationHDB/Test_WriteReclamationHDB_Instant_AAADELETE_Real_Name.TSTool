#@enabled False
# Disable because should only be testing against development database
#
# Test writing an instantaneous time series to ReclamationHDB database real table by specifying name, etc.
# The AAA_DELETE location data are written
StartLog(LogFile="Results/Test_WriteReclamationHDB_Instant_AAADELETE_Real_Name.TSTool.log")
RemoveFile(InputFile="Results/Test_ReclamationHDB_Instant_AAADELETE_Real_Name_out.dv",IfNotFound=Ignore)
# Make the definition of this time series match what is used in HDB so that the comparison
# of results all matches
NewPatternTimeSeries(Alias="AAA_DELETE",NewTSID="Real:AAA_DELETE.HDB.flow.Irregular",IrregularInterval=15Minute,Description="Real:AAA_DELETE",SetStart="2010-01-01 00:00",SetEnd="2010-01-03 12:30",Units="cfs",PatternValues="5,10,12,13,75")
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults\Test_WriteReclamationHDB_Instant_AAADELETE_Real_Name_out.dv")
# First write the time series to the database...
WriteReclamationHDB(DataStore="ReclamationHDB-ECO",SiteCommonName="AAA_DELETE",DataTypeCommonName="inst flow")
# Now read the same time series from the database using the period that was written...
Real:AAA_DELETE.HDB.inst flow.Irregular~ReclamationHDB-ECO
# Generate new output for what was read from the database...
WriteDateValue(OutputFile="Results/Test_WriteReclamationHDB_Instant_AAADELETE_Real_Name_out.dv",OutputStart="2010-01-01 00:00",OutputEnd="2010-01-03 12:30",TSList=LastMatchingTSID,TSID="Real:AAA_DELETE.HDB.flow.Irregular")
# Do regression comparison (allow 4 differences due to header differences)...
CompareFiles(InputFile1="ExpectedResults\Test_WriteReclamationHDB_Instant_AAADELETE_Real_Name_out.dv",InputFile2="Results/Test_WriteReclamationHDB_Instant_AAADELETE_Real_Name_out.dv",AllowedDiff="4",IfDifferent=Warn)
