# Test writing a Year ensemble of model time series to ReclamationHDB database:
# - development database
# - model table since ensemble
# - fabricated ensemble data
# - Year interval
# - SDI 100376 AAB_DELETE (average flow)
# - pick a short period
# - Write twice with different numbers to make sure old results are not just sitting
#   in the database.  Write non-missing values because HDB convention is to
#   NOT write missing values (not sure how re-write is handled if missing involved).
# - Use ReadReclamationHDB() and WriteRlamationHDB() because ensembles don't work with TSIDs
StartLog(LogFile="Results/Test_WriteReclamationHDB_Dev_Year_AAADELETE_Ensemble.TSTool.log")
RemoveFile(InputFile="Results/Test_ReclamationHDB_Dev_Year_AAADELETE_Ensemble.dv",IfNotFound=Ignore)
#
# Start first pass...
# First write the ensemble to the database...
NewPatternTimeSeries(Alias="AAA_DELETE_TS1",NewTSID="AAA_DELETE.HDB.flow.Year[2010]",Description="AAA_DELETE",SetStart="2010",SetEnd="2015",Units="cfs",PatternValues="5,10,12,13,75,33,14")
NewPatternTimeSeries(Alias="AAA_DELETE_TS2",NewTSID="AAA_DELETE.HDB.flow.Year[2011]",Description="AAA_DELETE",SetStart="2010",SetEnd="2015",Units="cfs",PatternValues="5.5,10.5,12.5,13.5,75.5,33.5,14.5")
NewEnsemble(TSList=AllMatchingTSID,TSID="AAA_DELETE_TS*",NewEnsembleID="AAA_DELETE",NewEnsembleName="AAA_DELETE")
WriteReclamationHDB(DataStore="ReclamationHDB-ECO-Dev",TSList=EnsembleID,EnsembleID="AAA_DELETE",SiteDataTypeID=100376,NewEnsembleName="AAA_DELETE Year Date",EnsembleTrace="%z",EnsembleModelName="NWS CBRFC Forecast",NewEnsembleModelRunDate="2013-10-02 10:11",ValidationFlag="P")
# Now read the same time series from the database using the period that was written...
ReadReclamationHDB(DataStore="ReclamationHDB-ECO-Dev",Interval="Year",SiteDataTypeID=100376,EnsembleName="AAA_DELETE Year Date",Alias="AAA_DELETE-Year-%z")
#
# Start second pass (previous values x 10)...
NewPatternTimeSeries(Alias="AAA_DELETE_TS3a",NewTSID="AAA_DELETE.HDB.flow.Year[2010]",Description="AAA_DELETE",SetStart="2010",SetEnd="2015",Units="cfs",PatternValues="50,100,120,130,750,330,140")
NewPatternTimeSeries(Alias="AAA_DELETE_TS3b",NewTSID="AAA_DELETE.HDB.flow.Year[2011]",Description="AAA_DELETE",SetStart="2010",SetEnd="2015",Units="cfs",PatternValues="50.5,100.5,120.5,130.5,750.5,330.5,140.5")
NewEnsemble(TSList=AllMatchingTSID,TSID="AAA_DELETE_TS3*",NewEnsembleID="AAA_DELETE2",NewEnsembleName="AAA_DELETE2")
# First write the time series to the database...
WriteReclamationHDB(DataStore="ReclamationHDB-ECO-Dev",TSList=EnsembleID,EnsembleID="AAA_DELETE2",SiteDataTypeID=100376,NewEnsembleName="AAA_DELETE Year Date",EnsembleTrace="%z",EnsembleModelName="NWS CBRFC Forecast",NewEnsembleModelRunDate="2013-10-02 10:11",ValidationFlag="P")
# Now read the same time series from the database using the period that was written...
ReadReclamationHDB(DataStore="ReclamationHDB-ECO-Dev",Interval="Year",SiteDataTypeID=100376,EnsembleName="AAA_DELETE Year Date",Alias="AAA_DELETE2-Year-%z")
#
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults/Test_WriteReclamationHDB_Dev_Year_AAADELETE_Ensemble.dv",TSList=AllMatchingTSID,TSID="AAA_DELETE2-Year*")
# Generate new output for what was read from the database...
WriteDateValue(OutputFile="Results/Test_WriteReclamationHDB_Dev_Year_AAADELETE_Ensemble.dv",TSList=AllMatchingTSID,TSID="AAA_Delete2-Year*")
# Do regression comparison...
CompareFiles(InputFile1="ExpectedResults\Test_WriteReclamationHDB_Dev_Year_AAADELETE_Ensemble.dv",InputFile2="Results/Test_WriteReclamationHDB_Dev_Year_AAADELETE_Ensemble.dv",IfDifferent=Warn)
