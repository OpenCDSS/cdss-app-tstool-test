# Test reading a FrostDateF32F year time series from ColoradoWaterHBGuest web service using a TSID
# Compare the resulting time series with that retrieved from HydroBase
StartLog(LogFile="Results/Test_TSIDColoradoWaterHBGuest_FrostDateF32F_Year.TSTool.log")
RemoveFile(InputFile="Results/Test_TSIDColoradoWaterHBGuest_FrostDateF32F_Year_out.dv",IfNotFound=Ignore)
# Read the same time series from the web service and HydroBase
SetInputPeriod(InputStart="1900",InputEnd="2005")
# Newer HydroBase uses GHCN identifer, Fort Collins
# 3005.NOAA.FrostDateF32F.Year~HydroBase
# 3005.NOAA.FrostDateF32F.Year~ColoradoWaterHBGuest
USC00053005.NOAA.FrostDateF28F.Year~HydroBase
USC00053005.NOAA.FrostDateF28F.Year~ColoradoWaterHBGuest
# Make sure that enough data are available in the test data, and some missing
CheckTimeSeriesStatistic(Statistic="NonmissingCount",CheckCriteria="<=",CheckValue1=10,IfCriteriaMet=Warn)
CheckTimeSeriesStatistic(Statistic="MissingCount",CheckCriteria="<=",CheckValue1=2,IfCriteriaMet=Warn)
# Uncomment the following command to regenerate the expected results file.
# The following does not work because numbers are slightly different and
# rounding on output produces different values.
#WriteDateValue(OutputFile="ExpectedResults/Test_TSIDColoradoWaterHBGuest_FrostDateF32F_Year_out.dv",Precision=2,TSList=FirstMatchingTSID,TSID="0300915.DWR.FrostDateF32F.Year")
#WriteDateValue(OutputFile="Results/Test_TSIDColoradoWaterHBGuest_FrostDateF32F_Year_out.dv",Precision=2,TSList=LastMatchingTSID,TSID="0300915.DWR.FrostDateF32F.Year")
#CompareFiles(InputFile1="ExpectedResults/Test_TSIDColoradoWaterHBGuest_FrostDateF32F_Year_out.dv",InputFile2="Results/Test_TSIDColoradoWaterHBGuest_FrostDateF32F_Year_out.dv",IfDifferent=Warn)
CompareTimeSeries(Tolerance=".01",WarnIfDifferent=True)
