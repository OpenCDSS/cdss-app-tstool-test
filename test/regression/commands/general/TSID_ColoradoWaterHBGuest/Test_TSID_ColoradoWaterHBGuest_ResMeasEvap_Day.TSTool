# Test reading a ResMeasEvap day time series from ColoradoWaterHBGuest web service using a TSID
# Compare the resulting time series with that retrieved from HydroBase
StartLog(LogFile="Results/Test_TSIDColoradoWaterHBGuest_ResMeasEvap_Day.TSTool.log")
RemoveFile(InputFile="Results/Test_TSIDColoradoWaterHBGuest_ResMeasEvap_Day_out.dv",IfNotFound=Ignore)
# Read the same time series from the web service and HydroBase
SetInputPeriod(InputStart="1990-01-01",InputEnd="2008-12-31")
0303732.DWR.ResMeasEvap.Day~HydroBase
0303732.DWR.ResMeasEvap.Day~ColoradoWaterHBGuest
# Make sure that enough data are available in the test data, and some missing
CheckTimeSeriesStatistic(Statistic="NonmissingCount",CheckCriteria="<=",CheckValue1=10,IfCriteriaMet=Warn)
CheckTimeSeriesStatistic(Statistic="MissingCount",CheckCriteria="<=",CheckValue1=2,IfCriteriaMet=Warn)
# Uncomment the following command to regenerate the expected results file.
# The following does not work because numbers are slightly different and
# rounding on output produces different values.
#WriteDateValue(OutputFile="ExpectedResults/Test_TSIDColoradoWaterHBGuest_ResMeasEvap_Day_out.dv",Precision=2,TSList=FirstMatchingTSID,TSID="0300915.DWR.ResMeasEvap.Day")
#WriteDateValue(OutputFile="Results/Test_TSIDColoradoWaterHBGuest_ResMeasEvap_Day_out.dv",Precision=2,TSList=LastMatchingTSID,TSID="0300915.DWR.ResMeasEvap.Day")
#CompareFiles(InputFile1="ExpectedResults/Test_TSIDColoradoWaterHBGuest_ResMeasEvap_Day_out.dv",InputFile2="Results/Test_TSIDColoradoWaterHBGuest_ResMeasEvap_Day_out.dv",IfDifferent=Warn)
# The following indicates differences because the web service seems to have
# quite a few extra zeros.
CompareTimeSeries(Tolerance=".01",WarnIfDifferent=True)
