# Test deleting table rows from a table
# - delete integers == a value
StartLog(LogFile="Results/Test_DeleteTableRows_int_eq.TSTool.log")
RemoveFile(InputFile="Results/Test_DeleteTableRows_int_eq_out.csv",IfNotFound=Ignore)
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\testtable.csv")
DeleteTableRows(TableID="Table1",Condition="integer1 == 2")
# Uncomment the following to generate the expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults\Test_DeleteTableRows_int_eq_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results\Test_DeleteTableRows_int_eq_out.csv")
# Do regression comparison with the original input
CompareFiles(InputFile1="ExpectedResults\Test_DeleteTableRows_int_eq_out.csv",InputFile2="Results/Test_DeleteTableRows_int_eq_out.csv",IfDifferent=Warn)
