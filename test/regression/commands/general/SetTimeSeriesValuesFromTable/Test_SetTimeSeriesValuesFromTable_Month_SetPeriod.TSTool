# Set time series data from a table
# - month interval
# - specify the set period using date/time strings
StartLog(LogFile="Results/Test_SetTimeSeriesValuesFromTable_Month_SetPeriod.TSTool.log")
RemoveFile(InputFile="Results/Test_SetTimeSeriesValuesFromTable_Month_SetPeriod_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1_Month",NewTSID="ts1...Month",Description="test data 1",SetStart="2000-01",SetEnd="2003-05",PatternValues="12,22,33,22,11,-999,55,11,-999,-999,-999,11")
NewPatternTimeSeries(Alias="ts2_Month",NewTSID="ts2...Month",Description="test data 2",SetStart="2001-01",SetEnd="2003-07",PatternValues="1,2,3,2,1,.5,5,1,5,15,4,1")
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\table.csv",Delimiter=",")
FormatTableDateTime(TableID="Table1",InputColumn="DateTime",DateTimeFormat="%Y-%m",OutputColumn="DateTime2",OutputType=DateTime,InsertBeforeColumn="Value")
SetTimeSeriesValuesFromTable(TSIDFormat="%L",SetStart="2000-03",SetEnd="2000-04",SetFlag="T",TableID="Table1",TableTSIDColumn="TSID",TableDateTimeColumn="DateTime2",TableValueColumn="Value")
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/Test_SetTimeSeriesValuesFromTable_Month_SetPeriod_out.dv")
WriteDateValue(OutputFile="Results/Test_SetTimeSeriesValuesFromTable_Month_SetPeriod_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_SetTimeSeriesValuesFromTable_Month_SetPeriod_out.dv",InputFile2="Results/Test_SetTimeSeriesValuesFromTable_Month_SetPeriod_out.dv",IfDifferent=Warn)
