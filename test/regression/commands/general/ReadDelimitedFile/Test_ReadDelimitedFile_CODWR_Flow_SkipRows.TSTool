# Test reading a State of Colorado tab-delimited flow and stage file
# Specify SkipRows for some header comment rows and some data rows
StartLog(LogFile="Results/Test_ReadDelimitedFile_CODWR_Flow_SkipRows.TSTool.log")
RemoveFile(InputFile="Results\Test_ReadDelimitedFile_CODWR_Flow_SkipRows_out.dv",IfNotFound=Ignore)
ReadDelimitedFile(InputFile="Data\CO-DWR-ADATUNCO-tab.txt",Delimiter="\t",ColumnNames="ID,DateTime,GAGE_HT,DISCHRG",DateTimeColumn="DateTime",ValueColumn="GAGE_HT,DISCHRG",SkipRows="1,4-6,21-25",SkipRowsAfterComments="1",LocationID="ADATUNCO",Provider="DWR",DataType="GAGE_HT,DISCHRG",Interval=15Minute,Units="ft,cfs",Alias="%L%T")
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults\Test_ReadDelimitedFile_CODWR_Flow_SkipRows_out.dv",Precision=2)
WriteDateValue(OutputFile="Results\Test_ReadDelimitedFile_CODWR_Flow_SkipRows_out.dv",Precision=2)
CompareFiles(InputFile1="ExpectedResults\Test_ReadDelimitedFile_CODWR_Flow_SkipRows_out.dv",InputFile2="Results\Test_ReadDelimitedFile_CODWR_Flow_SkipRows_out.dv",IfDifferent=Warn)
