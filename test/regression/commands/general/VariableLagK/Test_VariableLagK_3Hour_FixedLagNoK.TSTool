# Test fixed Lag and no attenuation (K)
SetDebugLevel(LogFileLevel=1)
StartLog(LogFile="Results/Test_VariableLagK_3Hour_FixedLagNoK.TSTool.log")
RemoveFile(InputFile="Results\Test_VariableLagK_3Hour_FixedLagNoK_out.dv",IfNotFound=Ignore)
#
# Read NWSCard input file
TS Inflow = NewPatternTimeSeries(NewTSID="TestLoc..SQIN.3Hour",SetStart="2000-01-01 00",SetEnd="2000-02-15 12",Units="CMS",PatternValues="1,2,4,6,6.5,7,5,3,1.5,1.1")
#
# Route using the same routing parameters used in the mcp3 input deck
# (metric units: Lag(hrs) K(hrs) Q(cms)
# Lag
# K
#     36.
#      0.
TS FixedLagNoK_OldLagK = LagK(TSID="Inflow",FillNearest=True,Lag=3,K=0)
VariableLagK(TSID="Inflow",NewTSID="TestLoc..SQIN.3Hour.routed",DataUnits=CMS,LagInterval=Hour,Lag="1e10,3",Alias="FixedLagNoK")
