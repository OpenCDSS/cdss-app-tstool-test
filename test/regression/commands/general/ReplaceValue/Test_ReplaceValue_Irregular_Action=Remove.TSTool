# Test removing negative values from an irregular interval time series
StartLog(LogFile="Results/Test_ReplaceValue_Irregular_Action=Remove.TSTool.log")
RemoveFile(InputFile="Results/Test_ReplaceValue_Irregular_Action=Remove_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1_irr",NewTSID="ts1...Irregular",IrregularInterval=Day,Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",PatternValues="1,2,3,2,1,-999,5,1,-999,-999,-999,1,3,5")
ReplaceValue(TSList=AllMatchingTSID,TSID="ts1_irr",MinValue=-1000,MaxValue=-998,Action=Remove)
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/Test_ReplaceValue_Irregular_Action=Remove_out.dv",Precision=0)
WriteDateValue(OutputFile="Results/Test_ReplaceValue_Irregular_Action=Remove_out.dv",Precision=0)
CompareFiles(InputFile1="ExpectedResults/Test_ReplaceValue_Irregular_Action=Remove_out.dv",InputFile2="Results/Test_ReplaceValue_Irregular_Action=Remove_out.dv",IfDifferent=Warn)
