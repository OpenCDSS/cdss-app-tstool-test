# Test reading a WaterClass day time series from ColoradoHydroBaseRest web service using a TSID
StartLog(LogFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Day_Without_Single_Quotes.TSTool.log")
RemoveFile(InputFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Day_Without_Single_Quotes_out.dv",IfNotFound=Ignore)
# Read the same time series from the web service and HydroBase
SetInputPeriod(InputStart="2017-09-19",InputEnd="2017-10-31")
0102433.DWR.WaterClass-0102433 S:8 F:0202362 U:A T:1 G:0102661 To:.Day~ColoradoHydroBaseRest
# Make sure that enough data are available in the test data, and some missing
CheckTimeSeriesStatistic(Statistic="NonmissingCount",CheckCriteria="<=",CheckValue1=10,IfCriteriaMet=Warn)
CheckTimeSeriesStatistic(Statistic="MissingCount",CheckCriteria="<=",CheckValue1=2,IfCriteriaMet=Warn)
# rounding on output produces different values.
WriteDateValue(TSList=FirstMatchingTSID,TSID="0102433.DWR.WaterClass-0102433 S:8 F:0202362 U:A T:1 G:0102661 To:.Day~ColoradoHydroBaseRest",OutputFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Day_Without_Single_Quotes_out.dv",Precision=2)