# Test reading an individual Telemetry D1 Hour time series from ColoradoHydroBaseRest web service using a TSID
StartLog(LogFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_D1_Hour.TSTool.log")
RemoveFile(InputFile="Results/Test_TSID_ColoradoWaterHydroBaseRest_Telemetry_D1_Hour_out.dv",IfNotFound=Ignore)
# Read a short period and visually compare with web service results
SetInputPeriod(InputStart="2018-01-01 00",InputEnd="2018-01-03 12")
abbrev:LONBLOCO.DWR.D1.Hour~ColoradoHydroBaseRest
# Uncomment the following line to regenerate expected results
# WriteDateValue(OutputFile="ExpectedResults/Test_TSID_ColoradoHydroBaseRest_Telemetry_D1_Hour_out.dv",Precision=2)
WriteDateValue(OutputFile="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_D1_Hour_out.dv",Precision=2)
CompareFiles(InputFile1="ExpectedResults/Test_TSID_ColoradoHydroBaseRest_Telemetry_D1_Hour_out.dv",InputFile2="Results/Test_TSID_ColoradoHydroBaseRest_Telemetry_D1_Hour_out.dv",IfDifferent=Warn)
