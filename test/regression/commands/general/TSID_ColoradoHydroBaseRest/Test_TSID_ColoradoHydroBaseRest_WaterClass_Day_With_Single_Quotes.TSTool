# Test reading a WaterClass day time series from ColoradoHydroBaseRest web service using a TSID
StartLog(LogFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Day_With_Single_Quotes.TSTool.log")
RemoveFile(InputFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Day_With_Single_Quotes_out.dv",IfNotFound=Ignore)
# Read the same time series from the web service and HydroBase
SetInputPeriod(InputStart="2016-06-28",InputEnd="2017-01-07")
0202235.DWR.'WaterClass-0202235 S:8 F:0200700.005 U:Q T: G:0202611 To:'.Day~ColoradoHydroBaseRest
# Make sure that enough data are available in the test data, and some missing
CheckTimeSeriesStatistic(Statistic="NonmissingCount",CheckCriteria="<=",CheckValue1=10,IfCriteriaMet=Warn)
CheckTimeSeriesStatistic(Statistic="MissingCount",CheckCriteria="<=",CheckValue1=2,IfCriteriaMet=Warn)
# rounding on output produces different values.
WriteDateValue(TSList=FirstMatchingTSID,TSID="0202235.DWR.'WaterClass-0202235 S:8 F:0200700.005 U:Q T: G:0202611 To:'.Day~ColoradoHydroBaseRest",OutputFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Day_With_Single_Quotes_out.dv",Precision=2)