# Test reading a WaterClass year time series from ColoradoHydroBaseRest web service using a TSID
StartLog(LogFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Year_With_Single_Quotes.TSTool.log")
RemoveFile(InputFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Year_With_Single_Quotes_out.dv",IfNotFound=Ignore)
# Read the same time series from the web service and HydroBase
SetInputPeriod(InputStart="2010",InputEnd="2017")
0102411.DWR.'WaterClass-0102411 S:3 F:0110319.001 U:A T: G:0102552 To:'.Year~ColoradoHydroBaseRest
# Make sure that enough data are available in the test data, and some missing
#CheckTimeSeriesStatistic(Statistic="NonmissingCount",CheckCriteria="<=",CheckValue1=10,IfCriteriaMet=Warn)
#CheckTimeSeriesStatistic(Statistic="MissingCount",CheckCriteria="<=",CheckValue1=2,IfCriteriaMet=Warn)
# rounding on output produces different values.
WriteDateValue(TSList=FirstMatchingTSID,TSID="0102411.DWR.'WaterClass-0102411 S:3 F:0110319.001 U:A T: G:0102552 To:'.Year~ColoradoHydroBaseRest",OutputFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_WaterClass_Year_With_Single_Quotes_out.dv",Precision=2)