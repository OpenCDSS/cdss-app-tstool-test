# Test reading a DivTotal year time series from ColoradoHydroBaseRest web service using a single TSID
StartLog(LogFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_DivTotal_Year.TSTool.log")
RemoveFile(InputFile="Results/Test_Individual_TSID_ColoradoHydroBaseRest_DivTotal_Year_out.dv",IfNotFound=Ignore)
# Read the same time series from the web service and HydroBase
