# Test the PreviousInclusive running statistic
# - ChangeFraction
# - with missing data
StartLog(LogFile="Results/Test_RunningStatisticTimeSeries_PreviousInclusive_ChangeFraction.TSTool.log")
RemoveFile(InputFile="Results/Test_RunningStatistic_PreviousInclusive_ChangeFraction_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1_day",NewTSID="ts1..Streamflow.Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",PatternValues="1,2,3,2,1,3,5,1,8,15,,1,3,5")
RunningStatisticTimeSeries(TSList=AllMatchingTSID,TSID="ts1_day",Statistic=ChangeFraction,SampleMethod=PreviousInclusive,Bracket=1,Alias="ts1_day_ChangeFraction")
# Uncomment the following command to regenerate expected results.
# WriteDateValue(TSList=AllMatchingTSID,TSID="ts1_day_ChangeFraction",OutputFile="ExpectedResults/Test_RunningStatisticTimeSeries_PreviousInclusive_ChangeFraction_out.dv")
WriteDateValue(TSList=AllMatchingTSID,TSID="ts1_day_ChangeFraction",OutputFile="Results/Test_RunningStatisticTimeSeries_PreviousInclusive_ChangeFraction_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_RunningStatisticTimeSeries_PreviousInclusive_ChangeFraction_out.dv",InputFile2="Results/Test_RunningStatisticTimeSeries_PreviousInclusive_ChangeFraction_out.dv",IfDifferent=Warn)
