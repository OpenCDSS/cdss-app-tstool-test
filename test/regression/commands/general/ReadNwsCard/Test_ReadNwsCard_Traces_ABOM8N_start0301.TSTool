# Test reading a card trace file from ESPADP that tests the case of the ESP
# run period being on the start and end of March (2 cases) to verify that
# Feb 29 leap year is handled and that the run can start on the end of the
# month (previously had a bug).
StartLog(LogFile="Results/Test_ReadNwsCard_Traces_ABOM8N_start0301.TSTool.log")
RemoveFile(InputFile="Results/Test_ReadNwsCard_Traces_ABOM8N_start0301_out.dv",IfNotFound=Ignore)
ReadNwsCard(InputFile="Data\ABOM8N_start0301.QA.ESP")
# Uncomment the following to regenerate the expected results...
# WriteDateValue(OutputFile="ExpectedResults/Test_ReadNwsCard_Traces_ABOM8N_start0301_out.dv")
WriteDateValue(OutputFile="Results/Test_ReadNwsCard_Traces_ABOM8N_start0301_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_ReadNwsCard_Traces_ABOM8N_start0301_out.dv",InputFile2="Results/Test_ReadNwsCard_Traces_ABOM8N_start0301_out.dv",IfDifferent=Warn)
