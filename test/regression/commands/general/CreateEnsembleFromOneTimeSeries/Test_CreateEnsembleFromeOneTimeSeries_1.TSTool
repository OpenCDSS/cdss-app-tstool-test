# Test creating an ensemble from one time series
# This CreateEnsembleFromOneTimeSeries command replaces the older CreateEnsemble command
StartLog(LogFile="Results/Test_CreateEnsembleFromOneTimeSeries_1.TSTool.log")
RemoveFile(InputFile="Results/Test_CreateEnsembleFromOneTimeSeries_1_out.dv",IfNotFound=Ignore)
TS ts1 = NewPatternTimeSeries(NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2007-05-13",PatternValues="1,2,4,3,5,6,25,10,8,5,1")
CreateEnsembleFromOneTimeSeries(TSID="ts1",TraceLength=1Year,EnsembleID="TestEnsemble",ReferenceDate="2000-01-01",ShiftDataHow=ShiftToReference)
# Uncomment the following command to regenerate expected results.
# WriteDateValue(OutputFile="ExpectedResults/Test_CreateEnsembleFromOneTimeSeries_1_out.dv")
WriteDateValue(OutputFile="Results/Test_CreateEnsembleFromOneTimeSeries_1_out.dv")
CompareFiles(InputFile1="ExpectedResults/Test_CreateEnsembleFromOneTimeSeries_1_out.dv",InputFile2="Results/Test_CreateEnsembleFromOneTimeSeries_1_out.dv",IfDifferent=Warn)
