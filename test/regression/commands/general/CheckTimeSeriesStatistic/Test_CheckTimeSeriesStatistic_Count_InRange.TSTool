# Check the Count statistic for InRange and warn (default behavior)
# Expected status is warning because the test detects a bad count
#@expectedStatus Warning
StartLog(LogFile="Results/Test_CheckTimeSeriesStatistic_Count_InRange.TSTool.log")
RemoveFile(InputFile="Results/Test_CheckTimeSeriesStatistic_Count_InRange_out.dv",IfNotFound=Ignore)
NewPatternTimeSeries(Alias="ts1",NewTSID="ts1...Day",Description="test data 1",SetStart="2000-01-01",SetEnd="2003-05-13",PatternValues="1,2,3,2,1,-999,5,1,-999,-999,-999,1,3,5")
NewPatternTimeSeries(Alias="ts2",NewTSID="ts2...Day",Description="test data 2",SetStart="2002-01-01",SetEnd="2003-07-28",PatternValues="11,22,33,22,11,-999,55,11,-999,-999,-999,11,33,55")
NewTable(TableID="Table1",Columns="TSID,string;Count,integer")
CheckTimeSeriesStatistic(Statistic="Count",TableID="Table1",TableTSIDColumn="TSID",TableStatisticColumn="Count",CheckCriteria="InRange",CheckValue1=500,CheckValue2=600,PropertyName="PeriodCheck",PropertyValue="Fail")
# Uncomment the following command to regenerate expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults/Test_CheckTimeSeriesStatistic_Count_InRange_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results/Test_CheckTimeSeriesStatistic_Count_InRange_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_CheckTimeSeriesStatistic_Count_InRange_out.csv",InputFile2="Results/Test_CheckTimeSeriesStatistic_Count_InRange_out.csv",IfDifferent=Warn)
