# Test writing HTML summary on monthly data for calendar year output...
StartLog(LogFile="Results/Test_WriteSummary_Month_HTML.TSTool.log")
RemoveFile(InputFile="Results/Test_WriteSummary_Month_HTML_out.html",IfNotFound=Ignore)
TS Alias = NewPatternTimeSeries(NewTSID="MyLoc..MyData.Month",Description="Test data",SetStart="1950-01",SetEnd="1955-03",Units="CFS",PatternValues="5,10,12,13,75,,,1",PatternFlags=",,,,a,b,,c")
# Uncomment the following command to regenerate the expected results file.
# WriteSummary(OutputFile="ExpectedResults/Test_WriteSummary_Month_HTML_out.html")
# Generate new output...
WriteSummary(OutputFile="Results/Test_WriteSummary_Month_HTML_out.html")
# Do regression comparison - allow 3 diffs for date/version/commmand line
CompareFiles(InputFile1="ExpectedResults\Test_WriteSummary_Month_HTML_out.html",InputFile2="Results/Test_WriteSummary_Month_HTML_out.html",AllowedDiff="3",IfDifferent=Warn)
