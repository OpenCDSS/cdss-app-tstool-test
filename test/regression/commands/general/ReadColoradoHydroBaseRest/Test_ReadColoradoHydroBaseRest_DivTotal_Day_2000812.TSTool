CompareFiles(InputFile1="ExpectedResults/Test_ReadColoradoHydroBaseRest_DivTotal_Day_2000812_out.dv",InputFile2="Results/Test_ReadColoradoHydroBaseRest_DivTotal_Day_2000812_out.dv",IfDifferent=Warn)
