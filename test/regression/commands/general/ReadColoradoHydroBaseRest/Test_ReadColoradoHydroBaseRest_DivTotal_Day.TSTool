#Test reading single time series using ReadColoradoHydroBaseRest and filtering by county and location
#Test results against TSID's generated from HydroBase database
StartLog(LogFile="Results/Test_ReadColoradoHydroBaseRest_DivTotal_Day.TSTool.log")
RemoveFile(InputFile="Results/Test_ReadColoradoHydroBaseRest_DivTotal_Day_out.dv",IfNotFound=Ignore)
#Read the time series using ReadColoradoHydroBaseRest and from HydroBase
SetInputPeriod(InputStart="1950-05-05",InputEnd="2000-05-05")
ReadColoradoHydroBaseRest(DataStore="ColoradoHydroBaseRest",DataType="DivTotal",Interval="Day",Where1="StructureWDID;Matches;0300915")
# 0300915 - CACHE LA POUDRE DITCH
ReadHydroBase(DataType="DivTotal",Interval="Day",Where1="District;=;3",Where2="Structure ID;=;915")
# Uncomment the following command to regenerate the expected results file.
# WriteDateValue(OutputFile="ExpectedResults/Test_ReadColoradHydroBaseRest_DivTotal_Day_out.dv")
WriteDateValue(OutputFile="Results/Test_ReadColoradHydroBaseRest_DivTotal_Day_out.dv")
0300915.DWR.DivTotal.Day~HydroBase
CompareTimeSeries(Tolerance=".01",WarnIfDifferent=True)
